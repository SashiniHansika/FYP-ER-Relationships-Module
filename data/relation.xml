<?xml version="1.0" encoding="utf-8"?>
<er>
	<relation name="is_sold" degree="binary" type="one_to_one">
		<member1 name="company" cardinality="one" primary_key="name"></member1>
		<member2 name="drug" cardinality="one" primary_key="trade_name"></member2>
	</relation>
	<relation name="has" degree="binary" type="one_to_many">
		<member1 name="patient" cardinality="many" primary_key="ssn"></member1>
		<member2 name="doctor" cardinality="one" primary_key="ssn"></member2>
	</relation>
	<relation name="sells" degree="binary" type="one_to_many">
		<member1 name="drug" cardinality="many" primary_key="trade_name"></member1>
		<member2 name="pharmacy" cardinality="one" primary_key="name"></member2>
	</relation>
	<relation name="prescribe" degree="binary" type="many_to_many">
		<member1 name="drug" cardinality="many" primary_key="trade_name"></member1>
		<member2 name="doctor" cardinality="many" primary_key="ssn"></member2>
	</relation>
	<relation name="have" degree="binary" type="many_to_many">
		<member1 name="pharmacy" cardinality="many" primary_key="name"></member1>
		<member2 name="company" cardinality="many" primary_key="name"></member2>
	</relation>
	<relation name="prescribe" degree="ternary" type="many_to_many_to_many">
		<member1 name="drug" cardinality="many" primary_key="trade_name"></member1>
		<member3 name="doctor" cardinality="many" primary_key="ssn"></member3>
		<member2 name="patient" cardinality="many" primary_key="ssn"></member2>
	</relation>
	<relation name="assume" degree="ternary" type="one_to_one_to_one">
		<member1 name="doctor" cardinality="one" primary_key="ssn"></member1>
		<member3 name="drug" cardinality="one" primary_key="trade_name"></member3>
		<member2 name="patient" cardinality="one" primary_key="ssn"></member2>
	</relation>
	<relation name="need" degree="unary" type="one_to_one">
		<member1 name="company" cardinality="one" primary_key="name"></member1>
		<member2 name="company" cardinality="one" primary_key="name"></member2>
	</relation>
</er>